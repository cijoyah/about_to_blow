<div id="form" class="wrapper_form">
  <%= form_for ([ @user, @track ]) do |f| %>
    
    <%= f.label :track_image %>
    <%= f.file_field :track_image %>

    <%= f.label :title %>
    <%= f.text_field :title %>

    <%= f.label :description %>
    <%= f.text_area :description %>

    <%= f.label :track %>
    <%= f.file_field :track %>

    <%= f.submit %>
  <% end %> 
</div>